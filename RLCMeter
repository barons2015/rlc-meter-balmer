	2.5 V, 1.5 V

	(R2+R3)/(R1+R2+R3) = 2.5/5
	(R3)/(R1+R2+R3) = 1.5/5

	R1 = 2.5
	R2 = 1
	R3 = 1.5

	R1 = 100 К
	R2 = 39 K
	R3 = 68 K
	U1 = 2.58 V
	U2 = 1.64 V

